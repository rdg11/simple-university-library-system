<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="133.0" prefWidth="259.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javathree.librarysystem.PrimaryController">
   <children>
      <Label text="Menu" />
      <Button fx:id="newMember" mnemonicParsing="false" onAction="#switchToNewMember" text="New Member" />
      <Button fx:id="removeMember" mnemonicParsing="false" onAction="#switchToRemoveMember" text="Remove Member" />
      <Button fx:id="newCollection" mnemonicParsing="false" onAction="#switchToNewCollection" text="New Collection" />
      <Button fx:id="removeCollection" mnemonicParsing="false" onAction="#switchToRemoveCollection" text="Remove Collection" />
      <Button fx:id="newEmployee" mnemonicParsing="false" onAction="#switchToNewEmployee" text="New Employee" />
      <Button fx:id="removeEmployee" mnemonicParsing="false" onAction="#switchToRemoveEmployee" text="Remove Employee" />
      <Button fx:id="newItem" mnemonicParsing="false" onAction="#switchToNewItem" text="New Item" />
      <Button fx:id="removeItem" mnemonicParsing="false" onAction="#switchToRemoveItem" text="Remove Item" />
      <Button fx:id="borrowItem" mnemonicParsing="false" onAction="#switchToBorrow" text="Borrow An Item" />
      <Button fx:id="returnItem" mnemonicParsing="false" onAction="#switchToReturn" text="Return An Item" />
      <Button fx:id="checkOverdues" mnemonicParsing="false" onAction="#switchToCheckOverdues" text="Check Overdues" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
