<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="133.0" prefWidth="259.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javathree.librarysystem.removeMemberController">
   <children>
      <Label text="Remove Member" />
      <TextField fx:id="memberID" promptText="Member ID" />
      <Text fx:id="message" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="submitRemoveMember" mnemonicParsing="false" onAction="#removeMember" text="Submit" />
          <Button mnemonicParsing="false" onAction="#returnRemoveMember" text="Return" />
        </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
